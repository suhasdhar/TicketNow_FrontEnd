{"ast":null,"code":"import CreateTicket from './components/CreateTicket.vue';\nimport HelloWorld from './components/HelloWorld.vue';\nimport NavBar from './components/NavBar.vue';\nexport default {\n  components: {\n    CreateTicket,\n    HelloWorld,\n    NavBar\n  },\n\n  data() {\n    return {\n      data: {\n        'suhas': 'Rajul123.'\n      },\n      displayTickets: true,\n      login: false,\n      // eslint-disable-next-line sort-keys\n      failed: false,\n      username: '',\n      // eslint-disable-next-line sort-keys\n      pass: '',\n      userID: ''\n    };\n  },\n\n  methods: {\n    disappearTickets(status) {\n      this.displayTickets = status;\n    },\n\n    async fun() {\n      let user = {};\n      await fetch('http://localhost:8080/ticket-now/auth/signin', {\n        body: JSON.stringify({\n          'password': this.pass,\n          'userId': this.userID\n        }),\n        headers: {\n          'Accept': 'application/json, text/plain',\n          'Content-Type': 'application/json;charset=UTF-8'\n        },\n        method: 'POST',\n        mode: 'cors'\n      }).then(res => res.json()).then(data => {\n        user = data;\n      }).then(() => {\n        if (user.usernameAndPasswordMatches) {\n          this.login = true;\n        }\n      }).catch(() => {\n        this.failed = true;\n        this.login = false;\n      }); // eslint-disable-next-line\n      // console.log(this.userID);\n    }\n\n  },\n  name: 'App'\n};","map":{"version":3,"mappings":"AAsBA,OAAOA,YAAP,MAAyB,+BAAzB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,MAAP,MAAmB,yBAAnB;AACA,eAAe;EACbC,UAAU,EAAE;IACVH,YADU;IAEVC,UAFU;IAGVC;EAHU,CADC;;EAMbE,IAAG,GAAK;IAEN,OAAO;MACLA,IAAI,EAAE;QACJ,SAAS;MADL,CADD;MAILC,cAAc,EAAE,IAJX;MAKLC,KAAK,EAAE,KALF;MAML;MACAC,MAAM,EAAE,KAPH;MAQLC,QAAQ,EAAE,EARL;MASL;MACAC,IAAI,EAAE,EAVD;MAWLC,MAAM,EAAE;IAXH,CAAP;EAeD,CAvBY;;EAwBbC,OAAO,EAAE;IACPC,gBAAe,CAAGC,MAAH,EAAW;MAExB,KAAKR,cAAL,GAAsBQ,MAAtB;IAED,CALM;;IAMP,MAAMC,GAAN,GAAa;MAEX,IAAIC,IAAG,GAAI,EAAX;