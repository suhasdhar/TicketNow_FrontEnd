{"ast":null,"code":"import HelloWorld from './components/HelloWorld.vue';\nexport default {\n  components: {\n    HelloWorld\n  },\n\n  data() {\n    return {\n      data: {\n        'suhas': 'Rajul123.'\n      },\n      login: false,\n      pass: '',\n      user: ''\n    };\n  },\n\n  methods: {\n    async fun() {\n      // eslint-disable-next-line\n      console.log(this.user);\n\n      if (this.pass === this.data.suhas) {\n        const response = await fetch('localhost:8080/', {\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          method: 'GET'\n        }); // eslint-disable-next-line\n\n        console.log(response);\n        this.login = true;\n      } else {\n        this.login = false;\n      }\n    }\n\n  },\n  name: 'App'\n};","map":{"version":3,"mappings":"AAcA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;;EAIbE,IAAG,GAAK;IAEN,OAAO;MACLA,IAAI,EAAE;QACJ,SAAS;MADL,CADD;MAILC,KAAK,EAAE,KAJF;MAKLC,IAAI,EAAE,EALD;MAMLC,IAAI,EAAE;IAND,CAAP;EAUD,CAhBY;;EAiBbC,OAAO,EAAE;IACP,MAAMC,GAAN,GAAa;MAEX;MACAC,OAAO,CAACC,GAAR,CAAY,KAAKJ,IAAjB;;MACA,IAAI,KAAKD,IAAL,KAAc,KAAKF,IAAL,CAAUQ,KAA5B,EAAmC;QAGjC,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAC1B,iBAD0B,EAE1B;UACEC,OAAO,EAAE;YACP,gBAAgB;UADT,CADX;UAKEC,MAAM,EAAE;QALV,CAF0B,CAA5B,CAHiC,CAejC;;QACAN,OAAO,CAACC,GAAR,CAAYE,QAAZ;QACA,KAAKR,KAAL,GAAa,IAAb;MAEF,CAnBA,MAmBO;QAEL,KAAKA,KAAL,GAAa,KAAb;MAEF;IAEF;;EA9BO,CAjBI;EAiDbY,IAAI,EAAE;AAjDO,CAAf","names":["HelloWorld","components","data","login","pass","user","methods","fun","console","log","suhas","response","fetch","headers","method","name"],"sourceRoot":"","sources":["/Users/suhasdhar/Desktop/Development/TicketNow/ticket_now/src/App.vue"],"sourcesContent":["<template>\n\n  <div>\n    <img  alt=\"Ticket Now\" src=\"./assets/ticket.png\" />\n    <HelloWorld msg=\"Ticket Now!\" />\n    <input type=\"text\" placeholder=\"username\" v-model=\"user\">  <input type=\"password\"  placeholder=\"password\"  v-model=\"pass\" name=\"\" id=\"\">\n    <button :onclick=\"fun\" text=\"login\">login</button>\n    <h1 v-if=\"login\" > logged in </h1>\n\n  </div>\n\n\n</template>\n<script>\nimport HelloWorld from './components/HelloWorld.vue';\nexport default {\n  components: {\n    HelloWorld,\n  },\n  data () {\n\n    return {\n      data: {\n        'suhas': 'Rajul123.'\n      },\n      login: false,\n      pass: '',\n      user: '',\n\n    };\n\n  },\n  methods: {\n    async fun () {\n\n      // eslint-disable-next-line\n      console.log(this.user);\n      if (this.pass === this.data.suhas) {\n\n\n        const response = await fetch(\n          'localhost:8080/',\n          {\n            headers: {\n              'Content-Type': 'application/json'\n            },\n\n            method: 'GET'\n\n\n          }\n        );\n        // eslint-disable-next-line\n        console.log(response);\n        this.login = true;\n\n      } else {\n\n        this.login = false;\n\n      }\n\n    }\n  },\n  name: 'App',\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\nimg {\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  padding: 5px;\n  width: 150px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}