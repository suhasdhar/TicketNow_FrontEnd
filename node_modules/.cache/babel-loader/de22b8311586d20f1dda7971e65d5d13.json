{"ast":null,"code":"export default {\n  data() {\n    return {\n      category: '',\n      create: null,\n      name: 'AdminPortal',\n      userid: '',\n      username: '',\n      validuser: null\n    };\n  },\n\n  methods: {\n    createUser() {\n      this.create = true;\n    },\n\n    deleteUser() {\n      this.create = false;\n    },\n\n    async validate() {\n      // eslint-disable-next-line\n      console.log(typeof this.userid);\n\n      if (this.userid === '') {\n        this.validuser = false;\n        return;\n      }\n\n      await fetch('http://localhost:8080/ticket-now/auth/validate-user', {\n        headers: {\n          'Accept': 'application/json, text/plain',\n          'Content-Type': 'application/json;charset=UTF-8',\n          'userid': this.userid\n        },\n        method: 'POST',\n        mode: 'cors'\n      }).then(res => res.json()).then(data => {\n        if (data) {\n          this.validuser = false; // eslint-disable-next-line\n\n          console.log(this.validuser);\n        } else {\n          this.validuser = true;\n        }\n      });\n    }\n\n  },\n\n  mounted() {\n    this.validuser = null;\n    this.create = null;\n  }\n\n};","map":{"version":3,"mappings":"AAmDA,eAAe;EAGbA,IAAG,GAAK;IAEN,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,MAAM,EAAE,IAFH;MAGLC,IAAI,EAAE,aAHD;MAILC,MAAM,EAAE,EAJH;MAKLC,QAAQ,EAAE,EALL;MAMLC,SAAS,EAAE;IANN,CAAP;EAUD,CAfY;;EAgBbC,OAAO,EAAE;IACPC,UAAS,GAAK;MAEZ,KAAKN,MAAL,GAAc,IAAd;IAED,CALM;;IAMPO,UAAS,GAAK;MAEZ,KAAKP,MAAL,GAAc,KAAd;IAED,CAVM;;IAWP,MAAMQ,QAAN,GAAkB;MAEhB;MACAC,OAAO,CAACC,GAAR,CAAY,OAAO,KAAKR,MAAxB;;MACA,IAAI,KAAKA,MAAL,KAAgB,EAApB,EAAwB;QAEtB,KAAKE,SAAL,GAAiB,KAAjB;QACA;MAEF;;MACA,MAAMO,KAAK,CACT,qDADS,EAET;QACEC,OAAO,EAAE;UAAC,UAAU,8BAAX;UACP,gBAAgB,gCADT;UAEP,UAAU,KAAKV;QAFR,CADX;QAIEW,MAAM,EAAE,MAJV;QAKEC,IAAI,EAAE;MALR,CAFS,CAAL,CASJC,IATI,CASEC,GAAD,IAASA,GAAG,CAACC,IAAJ,EATV,EAUJF,IAVI,CAUEjB,IAAD,IAAU;QAEb,IAAIA,IAAJ,EAAU;UAER,KAAKM,SAAL,GAAiB,KAAjB,CAFQ,CAGR;;UACCK,OAAO,CAACC,GAAR,CAAY,KAAKN,S